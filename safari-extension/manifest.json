{
  "manifest_version": 3,
  "name": "App Blocker",
  "version": "1.0.0",
  "description": "Blocks specified domains in Safari.",
  "permissions": ["storage", "activeTab", "tabs"],
  "action": {
    "default_title": "App Blocker (Extension)",
    "default_popup": "src/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"],
      "run_at": "document_start"
    }
  ]
}
